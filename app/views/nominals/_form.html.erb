<%= simple_form_for(@nominal) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :code, :readonly => f.object.persisted? %>
    <%= f.input :desc %>
    <%= f.input :isoffice %>
    <%= f.input :isclient %>
    <%= f.input :isdeposit %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
